      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            {
              "cooldown": "q.out_of_control"
            }
          ]
        },
        "cooldown": {
          "on_entry": [ "v.tick_delay = q.life_time;" ],
          "transitions": [
            {
              "default": "(q.life_time - v.tick_delay) >= 1.0"
            }
          ],
          "on_exit": [ "@s minecraft:entity_spawned" ]
        }
      }
    },

