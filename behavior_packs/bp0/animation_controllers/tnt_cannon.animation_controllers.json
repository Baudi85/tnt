          ]
        },
        "unsupported": {
          "on_entry": [
            "/titleraw @p actionbar {\"rawtext\":[{\"text\":\"§c\"},{\"translate\":\"tnt_cannon.unsupported\"}]}",
            "/tellraw @a[tag=!tnt_tip,r=12] {\"rawtext\":[{\"translate\":\"chat.objective.prefix\"},{\"translate\":\"chat.objective.list.shoot_structure_tip\"}]}",
            "/playsound note.chime @a[tag=!tnt_tip] ~~~",
            "/tag @a[r=12] add tnt_tip",

            "/playsound note.bass @a ~~~",
            "@s sndbx:remove_unsupported_tnt"
          ],
          "transitions": [
            {
              "default": "!q.is_illager_captain || !q.has_rider"
            }
          ]
        }
      }
