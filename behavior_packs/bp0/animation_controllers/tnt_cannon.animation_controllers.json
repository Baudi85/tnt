{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.tnt_cannon.detect_rider": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [ { "has_rider": "q.has_rider" }, { "has_no_rider": "!q.has_rider" } ]
        },
        "has_rider": {
          "on_entry": [ 
            "@s sndbx:has_rider",
            "/playsound tnt_cannon.activate @a ~~~ 1.0",
            "/execute @a[tag=!objective_shooting_range_ride_cannon] ~~~ function objectives/finished/shooting_range_ride_cannon"
          ],
          "transitions": [ { "default": "!q.has_rider" } ]
        },
        "has_no_rider": {
          "on_entry": [ "@s sndbx:has_no_rider", "/playsound tnt_cannon.deactivate @a ~~~ 1.0" ],
          "transitions": [ { "default": "q.has_rider" } ]
        }
      }
    },

    "controller.animation.tnt_cannon.detect_unloaded_launcher": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            { "unloaded_launcher": "q.has_rider && q.is_powered" }
          ]
        },
        "unloaded_launcher": {
          "on_entry": [
            "/titleraw @p actionbar {\"rawtext\":[{\"text\":\"§c\"},{\"translate\":\"tnt_cannon.unloaded\"}]}",
            "/tellraw @a[tag=!tnt_tip,r=12] {\"rawtext\":[{\"translate\":\"chat.objective.prefix\"},{\"translate\":\"chat.objective.list.shoot_structure_tip\"}]}",
            "/playsound note.chime @a[tag=!tnt_tip] ~~~",
            "/tag @a[r=12] add tnt_tip",
            "/playsound note.pling @p",

            "@s sndbx:remove_not_loaded"
          ],
          "transitions": [ { "default": "!q.is_powered" } ]
        }
      }
    },

